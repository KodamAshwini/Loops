import java.util.*;
public class Prime {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a number:");
		int n=sc.nextInt();
		int c=0;
		for(int i=2;i<n;i++) {
			if(n%i==0) {
				c++;
			}
		}
		if(c>0) {
			System.out.println("Not a Prime Number");
		}
		else {
			System.out.println("Prime Number");
		}

	}

}
